<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.softwork.ydk.middletermproject_time_table" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".GUI.TimeTableActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GUI.MakeTimeTableActivity" >
        </activity>

        <provider
            android:name=".Data.TTDBProvider"
            android:authorities="@string/authority"
            android:exported="true" />

        <activity android:name=".GUI.LectureActivity" >
        </activity>
        <activity android:name=".GUI.AdviceNoteActivity" >
        </activity>
        <activity android:name=".GUI.ChooseTimeTableActivity" >
        </activity>
    </application>

</manifest>
